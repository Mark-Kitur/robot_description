<?xml version="1.0"?>
<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro" >

  <xacro:property name="joint_effort" value="56.0"/>
  <xacro:property name="joint_velocity" value="2.792527"/>
  <xacro:property name="joint_damping" value="0.0"/>
  <xacro:property name="joint_friction" value="0.0"/>

  <xacro:property name="link1_mass" value="0.12"/>
  <xacro:property name="link2_mass" value="0.19"/>
  <xacro:property name="link3_mass" value="0.16"/>
  <xacro:property name="link4_mass" value="0.124"/>
  <xacro:property name="link5_mass" value="0.11"/>
  <xacro:property name="link6_mass" value="0.0739"/>
  <xacro:property name="flange_mass" value="0.035"/>

    <xacro:property name="M_PI" value="3.1415926535897931" />

    <xacro:property name="d435_cam_width" value="0.090"/>
    <xacro:property name="d435_cam_height" value="0.025"/>
    <xacro:property name="d435_cam_depth" value="0.02505"/>


  <xacro:macro name="link_inertial" params="mass ixx iyy izz">
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="${mass}"/>
      <inertia
        ixx="${ixx}" ixy="0.0" ixz="0.0"
        iyy="${iyy}" iyz="0.0"
        izz="${izz}"/>
    </inertial>
  </xacro:macro>

  <xacro:macro name="material_visual" params="ref_link ambient diffuse specular">
    <gazebo reference="${ref_link}">
      <visual>
        <material>
          <ambient>${ambient}</ambient>
          <diffuse>${diffuse}</diffuse>
          <specular>${specular}</specular>
        </material>
      </visual>
    </gazebo>
  </xacro:macro>

    <link name="base_link">
        <inertial>
            <origin xyz="0 0 0.0" rpy="0 0 0"/>
            <mass value="1"/>
            <inertia
                ixx="0.000784" ixy="0.0" ixz="0.0"
                iyy="0.000867" iyz="0.0"
                izz="0.001598"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="file://$(find robot_description)/meshes/g_shape_base_v2_0/visual/base_link.dae"/>
            </geometry>
            <origin xyz="0.0 0 -0.03" rpy="0 0 ${pi/2}"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.105 0.14 0.02"/>
            </geometry>
            <origin xyz="0.0 0.0 -0.015" rpy="0 0 0"/>
        </collision>
    </link>
    <gazebo reference="base_link">
        <visual>
            <material>
                <ambient>0.5 0.5 0.5 1</ambient>
                <diffuse>0.5 0.5 0.5 1</diffuse>
                <specular>0.5 0.5 0.5 1</specular>
            </material>
        </visual>
    </gazebo>


    <link name="link1">
      <xacro:link_inertial mass="${link1_mass}" ixx="0.000148" iyy="0.000148" izz="0.000096"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find robot_description)/meshes/mycobot_280/visual/link1.dae"/>
        </geometry>
        <origin xyz="0.0 0 0" rpy="0 0 ${-pi/2}"/>
      </visual>
      <collision>
        <geometry>
          <box size="0.08 0.12 0.075"/>
        </geometry>
        <origin xyz="0.0 -0.01 0.034" rpy="0 0 0"/>
      </collision>
    </link>

    <link name="link2">
      <xacro:link_inertial mass="${link2_mass}" ixx="0.000148" iyy="0.000148" izz="0.000096"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find robot_description)/meshes/mycobot_280/visual/link2.dae"/>
        </geometry>
        <origin xyz="0.0 0 -0.06096" rpy="0 0 ${-pi/2}"/>
      </visual>
      <collision>
        <geometry>
          <cylinder radius="0.025" length="0.085"/>
        </geometry>
        <origin xyz="0.0 0 -0.02" rpy="0 0 ${-pi/2}"/>
      </collision>
    </link>

    <link name="link3">
      <xacro:link_inertial mass="${link3_mass}" ixx="0.000148" iyy="0.000148" izz="0.000096"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find robot_description)/meshes/mycobot_280/visual/link3.dae"/>
        </geometry>
        <origin xyz="0.0 0 0.03256" rpy="0 ${-pi/2} 0"/>
      </visual>
      <collision>
        <geometry>
          <cylinder radius="0.025" length="0.15"/>
        </geometry>
        <origin xyz="-0.058 0 0.059" rpy="0 ${-pi/2} 0"/>
      </collision>
    </link>

    <link name="link4">
      <xacro:link_inertial mass="${link4_mass}" ixx="0.000103" iyy="0.000103" izz="0.000096"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find robot_description)/meshes/mycobot_280/visual/link4.dae"/>
        </geometry>
        <origin xyz="0.0 0 0.03056" rpy="0 ${-pi/2} 0"/>
      </visual>
      <collision>
        <geometry>
          <cylinder radius="0.025" length="0.125"/>
        </geometry>
        <origin xyz="-0.05 0 0.0" rpy="0 ${-pi/2} 0"/>
      </collision>
    </link>  

    <link name="link5">
      <xacro:link_inertial mass="${link5_mass}" ixx="0.000103" iyy="0.000103" izz="0.000096"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find robot_description)/meshes/mycobot_280/visual/link5.dae"/>
        </geometry>
        <origin xyz="0.0 0 -0.03356" rpy="${-pi/2} 0 0"/>
      </visual>
      <collision>
        <geometry>
          <box size="0.03 0.035 0.05"/>
        </geometry>
        <origin xyz="0.0 0 -0.005" rpy="0 0.0 0"/>
      </collision>
    </link>  

    <link name="link6">
      <xacro:link_inertial mass="${link6_mass}" ixx="0.00006649" iyy="0.00006649" izz="0.000096"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find robot_description)/meshes/mycobot_280/visual/link6.dae"/>
        </geometry>
        <origin xyz="0 0.00 -0.038" rpy="0 0 0"/>
      </visual>
      <collision>
        <geometry>
          <cylinder radius="0.025" length="0.06"/>
        </geometry>
        <origin xyz="0 0.00 0.0" rpy="${-pi/2} 0 0"/>
      </collision>
    </link>

    <link name="flange_link">
      <xacro:link_inertial mass="${flange_mass}" ixx="0.0000149" iyy="0.0000149" izz="0.0000196"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find robot_description)/meshes/mycobot_280/visual/link7.dae"/>
        </geometry>
        <origin xyz="0.0 0 -0.012" rpy="0 0 0"/>
      </visual>
      <collision>
        <geometry>
          <cylinder radius="0.02" length="0.02"/>
        </geometry>
        <origin xyz="0 0.00 0.0" rpy="0 0 0"/>
      </collision>
    </link>

    <link name="camera_link">
      <visual>
        <origin rpy="0 0 0"/>
        <geometry>
          <mesh filename="file://$(find robot_description)/meshes/d435/visual/d435.stl"/>
        </geometry>
        <material><color rgba="0.5 0.5 0.5 1"/></material>
      </visual>

      <collision>
        <geometry>
          <box size="${d435_cam_depth} ${d435_cam_width} ${d435_cam_height}"/>
        </geometry>
      </collision>

      <inertial>
        <mass value="0.072"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="0.00388" ixy="0" ixz="0" iyy="0.00049" iyz="0" izz="0.00387"/>
      </inertial>
    </link>

    <joint name="camera_link" type="fixed">
      <parent link="base_link"/>
      <child link="camera_link"/>
      <origin xyz="0 0 1" rpy=" 0 1.571 -1.571"/>

    </joint>


    <joint name="base_link_to_link1" type="fixed">
      <parent link="base_link"/>
      <child link="link1"/>
    </joint>

    <joint name="link1_to_link2" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="${joint_effort}" lower="-2.879793" upper="2.879793" velocity="${joint_velocity}"/>
      <parent link="link1"/>
      <child link="link2"/>
      <origin xyz="0 0 0.13156" rpy="0 0 ${pi/2}"/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <joint name="link2_to_link3" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="${joint_effort}" lower="-2.879793" upper="2.879793" velocity="${joint_velocity}"/>
      <parent link="link2"/>
      <child link="link3"/>
      <origin xyz="0 0 -0.001" rpy="0 ${pi/2} ${-pi/2}"/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <joint name="link3_to_link4" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="${joint_effort}" lower="-2.879793" upper="2.879793" velocity="${joint_velocity}"/>
      <parent link="link3"/>
      <child link="link4"/>
      <origin xyz="-0.1104 0 0" rpy="0 0 0"/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <joint name="link4_to_link5" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="${joint_effort}" lower="-2.879793" upper="2.879793" velocity="${joint_velocity}"/>
      <parent link="link4"/>
      <child link="link5"/>
      <origin xyz="-0.096 0 0.06062" rpy="0 0 ${-pi/2}"/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <joint name="link5_to_link6" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="${joint_effort}" lower="-2.879793" upper="2.879793" velocity="${joint_velocity}"/>
      <parent link="link5"/>
      <child link="link6"/>
      <origin xyz="0 -0.07318 0" rpy="${pi/2} ${-pi/2} 0"/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <joint name="link6_to_flange_link" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="${joint_effort}" lower="-3.05" upper="3.05" velocity="${joint_velocity}"/>
      <parent link="link6"/>
      <child link="flange_link"/>
      <origin xyz="0 0.0456 0" rpy="${-pi/2} 0 0"/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <xacro:material_visual ref_link="link1" ambient="0.5 0.5 0.5 1" diffuse="0.5 0.5 0.5 1" specular="0.5 0.5 0.5 1"/>
    <xacro:material_visual ref_link="link2" ambient="1 1 1 1" diffuse="1 1 1 1" specular="1 1 1 1"/>
    <xacro:material_visual ref_link="link3" ambient="1 1 1 1" diffuse="1 1 1 1" specular="1 1 1 1"/>
    <xacro:material_visual ref_link="link4" ambient="1 1 1 1" diffuse="1 1 1 1" specular="1 1 1 1"/>
    <xacro:material_visual ref_link="link5" ambient="1 1 1 1" diffuse="1 1 1 1" specular="1 1 1 1"/>
    <xacro:material_visual ref_link="link6" ambient="1 1 1 1" diffuse="1 1 1 1" specular="1 1 1 1"/>
    <xacro:material_visual ref_link="flange_link" ambient="0.5 0.5 0.5 1" diffuse="0.5 0.5 0.5 1" specular="0.5 0.5 0.5 1"/>
    <gazebo reference="camera_link">
      <sensor type="depth" name="realsense_depth">
        <update_rate>30</update_rate>

        <camera>
          <horizontal_fov>1.5184</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.05</near>
            <far>5.0</far>
          </clip>
        </camera>

        <plugin
          name="gazebo_ros_openni_kinect"
          filename="libgazebo_ros_camera.so">

          <always_on>true</always_on>
          <camera_name>camera</camera_name>

          <image_topic_name>camera/color/image_raw</image_topic_name>
          <camera_info_topic_name>camera/color/camera_info</camera_info_topic_name>

          <depth_image_topic_name>camera/depth/image_raw</depth_image_topic_name>
          <depth_camera_info_topic_name>camera/depth/camera_info</depth_camera_info_topic_name>

          <point_cloud_topic_name>camera/depth/color/points</point_cloud_topic_name>

          <frame_name>camera_head_depth_optical_frame</frame_name>
        </plugin>

      </sensor>
    </gazebo>

</robot>
